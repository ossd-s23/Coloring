{

  "manifest_version": 2,
  "name": "Coloring",
  "version": "1.0",

  "description": "Change background color and visibility of any page",

  "icons": {
    "48": "icons/icon-48.png"
  },

  "permissions": [
    "activeTab"
  ],

  "browser_action": {
    "default_icon": "icons/icon-32.png",
    "default_title": "ShadyNotes",
    "default_popup": "popup/notes.html"
    
  }
}
